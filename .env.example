# Server Configuration
PORT=3000
NODE_ENV=development

# One Id OAuth Configuration
ONEID_CLIENT_ID=your_client_id_here
ONEID_CLIENT_SECRET=your_client_secret_here
ONEID_AUTHORIZATION_URL=https://your-oneid-provider.com/oauth/authorize
ONEID_TOKEN_URL=https://your-oneid-provider.com/oauth/token
ONEID_USERINFO_URL=https://your-oneid-provider.com/oauth/userinfo
ONEID_REDIRECT_URI=http://localhost:3000/auth/callback

# Session Secret
SESSION_SECRET=your_random_session_secret_here_change_in_production
